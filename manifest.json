{
    "manifest_version": 2,
    "name": "Wikipedia Kit",
    "description": "",
    "version": "0.0",
    "content_scripts": [
        {
            "matches": ["https://*.wikipedia.org/wiki/*"],
            "css": ["wikipedia.css"],
            "js": ["wikipedia.js"]
        }
    ],
    "permissions": ["storage"],
    "web_accessible_resources": [
        "settings.html",
        "settings.js"
    ]
}